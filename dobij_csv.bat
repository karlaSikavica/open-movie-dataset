docker exec -u postgres filmoviContainer psql -d filmovi -c "COPY (SELECT f.film_id AS \"FilmID\", f.naziv AS \"Naziv filma\", f.godina AS \"Godina\", f.zanr AS \"Zanr\", f.zemlja AS \"Zemlja nastanka filma\", f.trajanje_min AS \"Trajanje filma\", f.redatelj_ime AS \"Ime redatelja\", f.redatelj_prezime AS \"Prezime redatelja\", f.budzet_mil_usd AS \"Budzet mil USD\", f.prosjecna_ocjena AS \"Prosjecna ocjena\", f.ocjena_imdb AS \"IMDB ocjena\", f.ocjena_rotten_tomatoes AS \"Rotten tomatoes ocjena\", f.ocjena_tmdb AS \"TMDB ocjena\", g.glumac_id AS \"GlumacID\", g.ime AS \"Ime glumca\", g.prezime AS \"Prezime glumca\", fg.uloga AS \"Uloga\", g.spol AS \"Spol\", g.datum_rodenja AS \"Datum rodenja\", g.drzava_podrijetla AS \"Drzava podrijetla\", g.aktivan_od AS \"Aktivan od\", g.broj_nagrada AS \"Broj nagrada\", g.nagrada_oscar AS \"Oscar\", g.nagrada_golden_globe AS \"Golden globe\", g.nagrada_bafta AS \"Bafta\" FROM film f JOIN film_glumac fg ON f.film_id = fg.film_id JOIN glumac g ON g.glumac_id = fg.glumac_id   ORDER BY f.film_id, g.glumac_id) TO STDOUT WITH (FORMAT CSV, HEADER)" > novi_csv.csv
